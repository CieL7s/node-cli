    "node-terminal",
    "interactive-cli"
  ],
  "author": {
    "name": "CieL7s",
    "url": "https://github.com/CieL7s"
  },
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CieL7s/node-cli.git"
  },
  "bugs": {
    "url": "https://github.com/CieL7s/node-cli/issues"
  },
  "homepage": "https://github.com/CieL7s/node-cli#readme",
  "dependencies": {},
  "devDependencies": {
    "javascript-obfuscator": "^4.0.2"
  },
  "bin": {
    "cielv7": "./cli.js"
  },
